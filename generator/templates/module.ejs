import { NgModule, ModuleWithProviders } from '@angular/core';
import {
    <%_ for(let i=0; i<model.length;i++){_%>
    <%= model[i]+'APIService'+((i<model.length-1)?',':'')%>
    <%_}_%>
 } from './services';
import { HttpClientModule } from '@angular/common/http';

@NgModule({
  imports: [
    HttpClientModule
  ],
  exports: [],
  declarations: [],
  providers: [
  ],
})
export class APIModule {
  public static forRoot(): ModuleWithProviders {
        return {
            ngModule: APIModule,
            providers: [
                <%_ for(let i=0; i<model.length;i++){_%>
                <%= model[i]+'APIService'+((i<model.length-1)?',':'')%>
                <%_}_%>
            ],
        };
    }
}
