{
  "name": "sw2ngx",
  "version": "3.0.2",
  "description": "Generate Angular API module form swagger.json",
  "main": "dist/sw2ngx.umd.js",
  "module": "dist/sw2ngx.es5.js",
  "typings": "dist/types/sw2ngx.d.ts",
  "scripts": {
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.js",
    "start": "rollup -c rollup.config.js -w"
  },
  "preferGlobal": true,
  "bin": {
    "sw2ngx": "./bin/sw2ngx"
  },
  "author": "https://github.com/YAZART",
  "license": "WTFPL",
  "homepage": "https://github.com/YAZART/sw2ngx#readme",
  "bugs": {
    "url": "https://github.com/YAZART/sw2ngx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YAZART/sw2ngx.git"
  },
  "keywords": [
    "swagger",
    "angular",
    "javascript",
    "generator",
    "ngx",
    "openapi",
    "ng2",
    "angular2",
    "angularX"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-inject": "^4.0.1",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/node": "^13.11.1",
    "@types/node-fetch": "^2.5.6",
    "@types/request": "^2.48.4",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "rollup": "^2.6.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "change-case": "^4.1.1",
    "node-fetch": "^2.6.0",
    "rimraf": "^3.0.2"
  }
}
