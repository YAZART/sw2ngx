{
  "name": "sw2ngx",
  "version": "5.0.0",
  "description": "Generate Angular API module form swagger.json",
  "main": "dist/sw2ngx.umd.js",
  "module": "dist/sw2ngx.es5.js",
  "typings": "dist/types/sw2ngx.d.ts",
  "scripts": {
    "lint": "eslint --cache --ext .ts src",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w"
  },
  "preferGlobal": true,
  "bin": {
    "sw2ngx": "./bin/sw2ngx"
  },
  "author": "https://github.com/YAZART",
  "license": "WTFPL",
  "homepage": "https://github.com/ngxutils/sw2ngx#readme",
  "bugs": {
    "url": "https://github.com/ngxutils/sw2ngx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngxutils/sw2ngx.git"
  },
  "keywords": [
    "swagger",
    "angular",
    "javascript",
    "generator",
    "ngx",
    "openapi",
    "ng2",
    "angular2",
    "angularX"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/node": "^13.11.1",
    "@types/request": "^2.48.4",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.4",
    "rollup": "^2.6.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/ejs": "^3.0.2",
    "@types/node-fetch": "^2.5.6",
    "change-case": "^4.1.1",
    "ejs": "^3.0.2",
    "node-fetch": "^2.6.1",
    "rimraf": "^3.0.2"
  }
}
