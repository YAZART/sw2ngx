{
  "name": "sw2ngx",
  "version": "3.0.0",
  "description": "Generate Angular API module form swagger.json",
  "main": "dist/sw2ngx.umd.js",
  "module": "dist/sw2ngx.es5.js",
  "typings": "dist/types/sw2ngx.d.ts",
  "scripts": {
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w"
  },
  "preferGlobal": true,
  "bin": {
    "sw2ngx": "./bin/sw2ngx"
  },
  "author": "https://github.com/YAZART",
  "license": "WTFPL",
  "homepage": "https://github.com/YAZART/sw2ngx#readme",
  "bugs": {
    "url": "https://github.com/YAZART/sw2ngx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YAZART/sw2ngx.git"
  },
  "keywords": [
    "swagger",
    "angular",
    "javascript",
    "generator",
    "ngx",
    "openapi",
    "ng2",
    "angular2",
    "angularX"
  ],
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/request": "^2.48.1",
    "@types/rimraf": "^2.0.2",
    "cross-env": "^5.0.1",
    "rollup": "^1.11.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.21.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "change-case": "^3.1.0",
    "request": "^2.88.0",
    "rimraf": "^2.6.2"
  }
}
